{
  "name": "json-schema-remote",
  "version": "1.6.2",
  "description": "Node.js module to validate JSON objects against a JSON Schema, including remote references ($ref)",
  "homepage": "https://github.com/entrecode/json-schema-remote",
  "author": {
    "name": "Ruben Deyhle",
    "email": "deyhle@entrecode.de",
    "url": "https://entrecode.de"
  },
  "scripts": {
    "test": "mocha -R spec --exit --timeout 5000",
    "build": "webpack json-schema-remote.js dist/json-schema-remote.js --output-library-target umd --output-library validator"
  },
  "repository": "entrecode/json-schema-remote",
  "license": "MIT",
  "keywords": [
    "json-schema-remote",
    "node",
    "javascript",
    "json-schema",
    "schema",
    "tv4",
    "validator"
  ],
  "main": "json-schema-remote.js",
  "typings": "json-schema-remote.d.ts",
  "dependencies": {
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "superagent": "^7.1.3",
