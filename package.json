    "tv4-formats": "^3.0.3",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "bin": {
    "json-schema-remote": "./bin/json-schema-remote.js"
  }
